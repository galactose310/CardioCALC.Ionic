<?xml version='1.0' encoding='utf-8'?>
<widget defaultlocale="fr-FR" id="fr.giccardio.cardiocalc" version="1.3.4" windows-packageVersion="1.3.4.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CardioCALC</name>
    <description>Calculateur médical spécialisé en cardiologie.</description>
    <author email="cardiocalc@outlook.com" href="http://ionicframework.com/">Olivier Raitiere</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <vs:toolsetVersion xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">6.3.1</vs:toolsetVersion>
    <vs:platformSpecificValues xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
        <vs:platformSpecificWidget platformName="windows" version="1.3.4.0">
            <vs:appxPackageIsForStore>True</vs:appxPackageIsForStore>
            <vs:appxBundle>Auto</vs:appxBundle>
            <vs:appxBundlePlatforms>arm</vs:appxBundlePlatforms>
            <vs:packageOutputPath>C:\Users\olivi\Documents\CardioCALC\CardioCALC\AppPackages\</vs:packageOutputPath>
        </vs:platformSpecificWidget>
    </vs:platformSpecificValues>
    <preference name="ScrollEnabled" value="false" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="windows-target-version" value="10.0" />
    <preference name="WindowsStoreDisplayName" value="CardioCALC" />
    <preference name="WindowsStoreIdentityName" value="36227OlivierRaitiere.CardioCALC" />
    <preference name="WindowsStorePublisherName" value="Olivier Raitiere" />
</widget>
